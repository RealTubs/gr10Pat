unit AppController;

interface

uses
  SysUtils, Classes, Forms;

type
  TAppController = class
  private
    // FMainForm: TForm4
  public
    procedure HandleControlAction(Sender: Tobject);
    constructor Create(AOwner: TComponent); override;
    destructor Destroy; override;
